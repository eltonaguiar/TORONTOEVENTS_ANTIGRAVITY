{
  "name": "toronto-events",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:sftp": "cross-env DEPLOY_TARGET=sftp next build",
    "fetch:github": "tsx scripts/fetch-github-data.ts",
    "build:sftp:github": "npm run fetch:github && npm run build:sftp",
    "start": "next start",
    "lint": "next lint",
    "scrape": "npx tsx scripts/run-scraper.ts",
    "deploy": "gh-pages -d build -t",
    "deploy:sftp": "npm run build:sftp && tsx scripts/deploy-simple.ts",
    "deploy:sftp:github": "npm run build:sftp:github && tsx scripts/deploy-simple.ts",
    "refresh:all": "tsx scripts/refresh-and-deploy.ts",
    "sync:deploy": "tsx scripts/sync-and-deploy.ts"
  },
  "dependencies": {
    "axios": "^1.13.3",
    "basic-ftp": "^5.1.0",
    "cheerio": "^1.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "fake-useragent": "^1.0.1",
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "ssh2-sftp-client": "^12.0.1"
  },
  "devDependencies": {
    "@types/fake-useragent": "^1.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
